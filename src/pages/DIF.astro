---
import Layout from '../layouts/Layout_dif.astro'
const title = 'H. Ayuntamiento de Esc√°rcega';


const response = await fetch('https://pokeapi.co/api/v2/ability/?limit=20&offset=0');
const data = await response.json();
const pokemons = data.results;

---

<Layout title={title}>
    <h1 class="pt-[120px]">Estoy en la pagina del DIF</h1>
<ul>
    {pokemons.map(pokemon => (<li>{pokemon.name}</li>))}
</ul>
</Layout>